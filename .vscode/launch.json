                "serviceAccountName": "992639108971-compute@developer.gserviceaccount.com",
                "cloudSqlInstances": [
                    "ewallet-laravel-db"
                ],
                "resources": {
                    "limits": {
                        "cpu": 1,
                        "memory": "256Mi"
                    }
                }
            },
            "target": {
                "minikube": {}
            },
            "watch": true,
            "externalPortForward": 8080
        }
    ]
